# Netlify environment variables
# Copy these variables to your Netlify site's environment variables settings

# Required for Cloudflare integration
NODE_VERSION=18.18.0
PNPM_VERSION=9.4.0
NODE_OPTIONS=--max_old_space_size=4096

# Set this to enable edge functions properly
NETLIFY_EDGE_FUNCTIONS=true

# Add your API keys below (at least one is required for the app to function)
# GROQ_API_KEY=your_key_here
# OPENAI_API_KEY=your_key_here
# ANTHROPIC_API_KEY=your_key_here
# OPEN_ROUTER_API_KEY=your_key_here
# GOOGLE_GENERATIVE_AI_API_KEY=your_key_here
# OLLAMA_API_BASE_URL=your_url_here
# XAI_API_KEY=your_key_here
# TOGETHER_API_KEY=your_key_here
# TOGETHER_API_BASE_URL=your_url_here

# Logging level
VITE_LOG_LEVEL=debug